{
  "name": "URL Saver",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "sidePanel",
    "storage",
    "contextMenus"
  ],
  "action": {
    "default_title": "Open URL Saver"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "web_accessible_resources": [{
    "resources": ["sidepanel.html", "sidepanel.js", "sidepanel.css"],
    "matches": ["<all_urls>"]
  }]
}
