{
  "name": "URL Saver",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "sidePanel",
    "storage",
    "contextMenus"
  ],
  "action": {
    "default_title": "Open URL Saver"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  }
}
